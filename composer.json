{
    "name": "danack/blog",

    "description": "The start of project Blog",

    "require-dev": {
        "mockery/mockery": "0.9.1",
        "phpunit/phpunit": "4.4.5"
    },

    "require":{
        "amphp/artax": "1.0.0-rc6",
        "behat/mink": "~1.6.1",
        "behat/mink-goutte-driver": "~1.1.0", 
        "danack/console": "^2.6.0",
        "danack/githubartaxservice": "^0.4.6",
        "danack/jig": "^0.16.1",
        "danack/syntaxhighlighter": "~3.1.0",
        "danack/tier": "0.1.4",
        "doctrine/common": "2.5.0",
        "fluentdom/fluentdom": "v4.1.3",
        "incoming/incoming": "v0.2.1",
        "intahwebz/db": "^0.1.1",
        "intahwebz/weaver": "~0.4.5",
        "intahwebz/configurator": "^0.2.8",
        "leafo/lessphp": "0.4.0",
        "leafo/scssphp": "0.1.1",
        "michelf/php-markdown": "~1.3",
        "mikey179/vfsStream": "1.4.0",
        "nikic/fast-route": "0.6.0",
        "openpsa/universalfeedcreator": "v1.8.0",
        "oyejorge/less.php": "v1.7.0.1",
        "predis/predis": "^1.0.1",
        "rdlowrey/auryn": "^1.0.2",
        "room11/caching": "^0.0.4", 
        "room11/http": "^0.0.3",
        "squizlabs/php_codesniffer": "^2.0.0",
        "zendframework\/zend-inputfilter": ">=2.1.5",
        "zendframework\/zend-filter": ">=2.1.5",
        "zendframework\/zend-validator": ">=2.1.5",
        "zendframework/zend-permissions-acl": "^2.1.0",
        "zendframework/zend-stdlib": "~2.5"
    },
    "autoload": {
        "psr-0": {
            "BaseReality": "lib/",
            "Intahwebz": "lib/",
            "Blog": ["lib/", "./var/compile/"],
            "FileFilter": ["./lib/" ],
            "FilePacker": ["./lib/" ],
            "ScriptServer": ["./lib/" ],
            "Jig": ["./var/compile/" ]
        }
    },
    "prefer-stable": true
}
